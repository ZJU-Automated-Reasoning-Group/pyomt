(declare-fun x () Real)
(declare-fun y () Real)
(assert (>= x 0))
(assert (<= x 6))
(assert (> y 0))
(assert (> (- y (/ x 8)) 0))
(assert (= (* 9 (- y (/ x 8)) (- y (/ x 8))) (+ (* (- 6 x) (- 6 x)) 4)))
(minimize y)
(check-sat)
(get-objectives)
(exit)
